adDocuments();\n  },\n  methods: {\n    async loadDocuments() {\n      this.loading = true;\n      this.error = null;\n      this.documents = [];\n\n      try {\n        let data = await API.post('documents/admin/list');\n        // sort by createdAt. newest above.\n        this.documents = data.data.sort(\n          (a, b) => new Date(b.createdAt) - new Date(a.createdAt)\n        );\n      } catch (err) {\n        this.error = err.message;\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped></style>\n"]}]}